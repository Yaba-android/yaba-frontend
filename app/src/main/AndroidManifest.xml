<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.github.nasrat_v.maktaba_android_frontend_mvp">

    <application
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="GoogleAppIndexingWarning">
        <activity
                android:name=".Activity.RecommendedActivity"
                tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".Activity.BrowseActivity"
                tools:ignore="InnerclassSeparator">
        </activity>
        <activity
                android:name=".Activity.LibraryActivity"
                tools:ignore="InnerclassSeparator">
        </activity>
        <activity
                android:name=".Activity.BookDetailsActivity"
                tools:ignore="InnerclassSeparator">
        </activity>
        <activity
                android:name=".Activity.PopularSpeciesActivity"
                tools:ignore="InnerclassSeparator">
        </activity>
        <activity
                android:name=".Activity.GroupActivity"
                tools:ignore="InnerclassSeparator">
        </activity>
    </application>

</manifest>